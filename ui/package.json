{
  "name": "todo-app",
  "version": "1.0.0",
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "esbuild": "0.27.1",
    "lit": "3.3.1",
    "typescript": "5.9.3"
  },
  "scripts": {
    "analyze": "npx biome check ./src",
    "format": "npx biome check --write ./src",
    "typecheck": "npx tsc",
    "build": "npm run typecheck && npx esbuild --bundle --outdir=dist --minify --keep-names --format=esm src/index.ts"
  }
}
